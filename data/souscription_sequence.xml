<odoo>
    <data noupdate="1">
        <record id="seq_souscription" model="ir.sequence">
            <field name="name">Souscription</field>
            <field name="code">souscription.sequence</field>
            <field name="prefix">S</field>
            <field name="padding">4</field>
            <!-- number_next permet de faire démarrer la séquence à une autre valeur. ex 500-->
            <field name="number_next">1</field> 
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>